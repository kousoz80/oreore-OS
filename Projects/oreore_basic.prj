   if xk+2$=&apos;m&apos;   then modefy
   if xk+2$=&apos;;&apos;     then etext#, xxtext#= li#, xxli#= serch
   if xk+2$=&apos;:&apos;     then etext#, xxtext#= li#, xxli#= serch_next
