<codetext>// プログラムを消去する
clear_program:
  TopProg, prog#=
  
  clear_program1:
    if prog#=NULL goto clear_program2
    prog#, pp#=
    pp#, -&gt;Program.next# prog#=
    pp#, -&gt;Program.text# free
    pp#, free
    goto clear_program1
  clear_program2:
  NULL, TopProg#= EndProg#=
  end
</codetext>
