//  &quot;symbol char:&quot;, prints nl
  
pass2_getToken5:
  (TokenP)$, is_symbol_char tt#=
  if tt#=1 then  (TokenP)$, TokenText$(ii#)= TokenP#++ ii#++ gotopass2_getToken5
  NULL, TokenText$(ii#)=

//  &quot;TokenText=&quot;, prints TokenText, prints nl

    // &quot;else&quot;キーワードが出てきたら行の終わりと判断する
