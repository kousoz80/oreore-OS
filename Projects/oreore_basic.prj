<codetext>// 算術式の処理
eval_aexpression:

// &quot;eval aexpression:&quot;, prints nl

  // 項を解析
  eval_term
  value_type tt#=
  if tt#&lt;&gt;STRING goto eval_aexpression2

  // 文字列型の場合
