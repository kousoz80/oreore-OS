// コマンド（１文字）入力
get_command:
  display
   0, eflg$=
   1, ll#=

// コマンドタイプ０：数字（パラメータ）
get_command0:
   xk#, k2#=
   inkey xk#=
   if xk#=0     goto get_command0
   if xk#=k2# goto get_command0

   if xk+2$&lt;&apos;0&apos; goto get_command1
   if xk+2$&gt;&apos;9&apos; goto get_command1
   if eflg$=0 then 0, ll#=
   ll#, 10, * xk+2$, + &apos;0&apos;, - ll#=
   1,  eflg$=
   goto get_command0

// コマンドタイプ１：テキストが空の時は無効
