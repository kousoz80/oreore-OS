get_command2:
   if xk+2$=&apos;i&apos;    then li#, xxli#= insert
   if xk+2$=&apos;a&apos;   then li#, xxli#= 1, ll#= jump_foward nl insert
   if xk+2$=&apos;r&apos;   then nl erase_line  read_file
