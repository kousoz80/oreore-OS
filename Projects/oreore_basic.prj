   if tail#=xbuf# goto get_command2
   if xk+2$=&apos;s&apos;    then etext#, copy_p#= 
   if xk$=2      then xjump_foward  // 下矢印キー
   if xk+2$=&apos;j&apos;     then xjump            
   if xk+2$=&apos;.&apos;     then xjump_end
   if xk$=1      then xjump_reverse  // 上矢印キー
