  // 原子を解析
  eval_atom

  // べき乗算は数値型にのみ適用される
  value_type tt#=
  if tt#&lt;&gt;NUMBER then  0, end

eval_factor1:

      // 因子は原子^原子
      if TokenText$=&apos;^&apos; then getToken eval_atom eval_power gotoeval_factor1
// &quot;eval factor end:&quot;, prints nl
      0, end


